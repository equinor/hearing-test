{
  "name": "hearingtest",
  "private": true,
  "version": "0.2.7",
  "description": "Hearing Test - Customer: Waiter, waiter! There's a hearing aid in my soup. Waiter: Did you say anything?",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ipad": "react-native run-ios --simulator='iPad Retina'",
    "ipadair": "react-native run-ios --simulator 'iPad Air 2'",
    "iphone7": "react-native run-ios --simulator='iPhone7'",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:save": "jest --collectCoverage --json --outputFile=test-results.json",
    "prepare": "husky install"
  },
  "repository": "https://github.com/equinor/mad-project-hearing-test-client",
  "keywords": [],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@types/react-native": "^0.64.10",
    "@types/react-test-renderer": "^17.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.9.0",
    "detox": "^17.14.6",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.5.1",
    "fetch-mock": "^6.0.0-beta.9",
    "husky": "^7.0.0",
    "jasmine-reporters": "^2.3.2",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^1.4.0",
    "lint-staged": "^10.0.7",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "1.18.2",
    "react-dom": "^16.12.0",
    "react-test-renderer": "16.8.3",
    "rnpm-plugin-windows": "^0.2.11",
    "schedule": "^0.4.0",
    "sinon": "^4.2.2",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^4.2.2",
    "ajv": "^6.0.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "lodash": "^4.17.15",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.0",
    "react": "16.8.3",
    "react-native": "^0.59.10",
    "react-native-cookies": "^3.3.0",
    "react-native-device-info": "2.3.2",
    "react-native-image-modal": "^1.0.16",
    "react-native-keyboard-aware-scrollview": "^2.1.0",
    "react-native-ms-adal": "^0.4.0",
    "react-native-salute": "github:equinor/react-native-salute#4ffda15d9910de5fb0274e218c15b8e6f6ffdaeb",
    "react-native-sound": "^0.11.0",
    "react-native-splash-screen": "3.0.6",
    "react-native-swipe-gestures": "^1.0.4",
    "react-native-system-setting": "^1.7.4",
    "react-native-trustkit-wrapper": "^0.0.8",
    "react-native-vector-icons": "6.6.0",
    "react-native-windows": "^0.59.0-rc.3",
    "react-navigation": "^2.17.0",
    "react-redux": "^5.0.6",
    "redux": "3.6.0",
    "redux-actions": "^2.2.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-persist": "4.10.2",
    "redux-persist-migrate": "^5.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "shortid": "^2.2.8",
    "url": "^0.11.0"
  },
  "jest": {
    "preset": "react-native",
    "setupTestFrameworkScriptFile": "<rootDir>/testSetup.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e/"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/hearingtest.app",
        "build": "xcodebuild -workspace ios/madapp.xcworkspace -scheme madapp -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 11"
      }
    },
    "test-runner": "jest"
  },
  "lint-staged": {
    "*.tsx": [
      "eslint --fix"
    ],
    "*.ts": [
      "eslint --fix"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "rnpm": {
    "assets": [
      "./app/assets/fonts/"
    ]
  }
}
